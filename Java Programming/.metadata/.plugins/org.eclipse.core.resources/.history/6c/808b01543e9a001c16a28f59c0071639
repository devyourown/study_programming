package treeandgraph;

import java.util.LinkedList;

public class Node {
	public LinkedList<Integer> adjcent;
	public String name;
	public int index;
	
	public Node(int index) {
		this.index = index;
		adjcent = new LinkedList<>();
	}
	
	public void addNeighbor(int otherIndex) {
		adjcent.add(otherIndex);
	}
}
